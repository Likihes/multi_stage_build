pipeline {
    agent any
    stages { 
        stage('gitcheckout') {
            steps {
                checkout scmGit(
                    branches: [[name: '*/main']], 
                    userRemoteConfigs: [[
                        credentialsId: 'github_creditial', 
                        url: 'https://github.com/Likihes/multi_stage_build.git'
                        ]])
            }
        }
    }
}